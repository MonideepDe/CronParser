    "version": "0.2.0",
    "configurations": [
        {
            "name": "CronParser",
            "type": "python",
            "request": "launch",
            "program": "src/main.py",
            "args": [
                "*/15 0 1,15 * 1,4,2,4-6/2 /usr/bin/find"
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "PYTHONPATH": "${workspaceRoot}"
            }
        }
    ]
}